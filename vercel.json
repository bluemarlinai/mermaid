{
  "version": 2,
  "name": "mermaid-online-editor",
  "builds": [
    {
      "src": "**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "^/styles\\.css",
      "dest": "/styles.css"
    },
    {
      "src": "^/app\\.js",
      "dest": "/app.js"
    },
    {
      "src": "^/robots\\.txt",
      "dest": "/robots.txt"
    },
    {
      "src": "^/sitemap\\.xml",
      "dest": "/sitemap.xml"
    },
    {
      "src": "^/test\\.html",
      "dest": "/test.html"
    },
    {
      "src": "^/(.*)",
      "dest": "/index.html"
    }
  ]
}